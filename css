import { MatDatepickerModule } from '@angular/material/datepicker';
import { MatNativeDateModule, DateAdapter, NativeDateAdapter } from '@angular/material/core';



 imports: [
    MatDatepickerModule,
    MatNativeDateModule, // Provides the NativeDateAdapter
  ],
  providers: [
    { provide: DateAdapter, useClass: NativeDateAdapter }, // Inject NativeDateAdapter
  ],
